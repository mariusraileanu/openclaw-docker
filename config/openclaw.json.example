{
  "meta": {
    "lastTouchedVersion": "2026.2.15",
    "lastTouchedAt": "2026-02-13T15:12:43.378Z"
  },
  "models": {
    "mode": "merge",
    "providers": {
      "compass": {
        "baseUrl": "https://api.core42.ai/v1",
        "apiKey": "${COMPASS_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "gpt-5.1",
            "name": "Compass GPT-5.1",
            "reasoning": false,
            "input": [
              "text"
            ],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "compass/gpt-5.1"
      },
      "models": {
        "google/gemini-2.5-flash": {},
        "openai/gpt-5.2": {
          "alias": "gpt"
        },
        "compass/gpt-5.1": {
          "alias": "compass"
        },
        "openrouter/arcee-ai/trinity-large-preview:free": {
          "alias": "trinity"
        }
      },
      "workspace": "/home/node/workspace",
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      },
      "sandbox": {
        "mode": "off",
        "workspaceAccess": "rw"
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "workspace": "/home/node/workspace",
        "agentDir": "/home/node/.openclaw/agents/main/agent",
        "model": "compass/gpt-5.1"
      },
      {
        "id": "cron",
        "workspace": "/home/node/.openclaw/workspace-cron",
        "agentDir": "/home/node/.openclaw/agents/cron/agent",
        "model": "compass/gpt-5.1"
      }
    ]
  },
  "tools": {
    "deny": [
      "apply_patch",
      "process",
      "nodes",
      "canvas"
    ],
    "web": {
      "search": {
        "enabled": false
      },
      "fetch": {
        "enabled": false
      }
    },
    "exec": {
      "host": "gateway",
      "security": "allowlist",
      "ask": "on-miss",
      "pathPrepend": [
        "/home/node/workspace/bin",
        "/home/node/.openclaw/skills/tavily-search/scripts",
        "/home/node/.openclaw/skills/whoop-central/scripts",
        "/home/node/.openclaw/skills/weather/scripts"
      ],
      "safeBins": [
        "graph-mcp",
        "graph-mcp-write",
        "summarize",
        "tavily-search",
        "tavily-extract",
        "whoop-central",
        "curl",
        "weather",
        "playwright-mcp"
      ]
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "groupPolicy": "disabled",
      "streamMode": "partial"
    },
    "whatsapp": {
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist",
      "allowFrom": [],
      "groupAllowFrom": []
    },
    "signal": {
      "enabled": false,
      "account": "",
      "cliPath": "signal-cli",
      "dmPolicy": "pairing",
      "allowFrom": [],
      "groupPolicy": "allowlist",
      "groupAllowFrom": []
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_AUTH_TOKEN}"
    },
    "remote": {
      "token": "${OPENCLAW_GATEWAY_AUTH_TOKEN}"
    }
  },
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      },
      "whatsapp": {
        "enabled": true
      },
      "signal": {
        "enabled": false
      }
    }
  },
  "browser": {
    "enabled": true,
    "defaultProfile": "openclaw",
    "attachOnly": false,
    "headless": true,
    "noSandbox": true
  }
}
